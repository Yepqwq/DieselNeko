  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "dist"
  ],
  "author": "Shigma <shigma10826@gmail.com>",
  "scripts": {
    "build": "tsc -b && koishi-console build"
  },
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koishijs/koishi-plugin-iframe.git"
  },
  "bugs": {
    "url": "https://github.com/koishijs/koishi-plugin-iframe/issues"
  },
  "homepage": "https://github.com/koishijs/koishi-plugin-iframe",
  "keywords": [
    "bot",
    "chatbot",
    "koishi",
    "plugin",
    "iframe",
    "router",
    "console",
    "browser"
