  "peerDependencies": {
    "@koishijs/plugin-console": "^5.11.10",
    "koishi": "^4.13.8"
  },
  "devDependencies": {
    "@koishijs/client": "^5.11.10",
    "@koishijs/plugin-console": "^5.11.10",
    "@types/node": "^20.4.2",
    "esbuild": "^0.18.14",
    "esbuild-register": "^3.4.2",
    "koishi": "^4.13.8",
    "typescript": "^5.1.6"
  }
